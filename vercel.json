{
    "version":2,
    "builds":[
        {
            "src":"core/wsgi.py",
            "user":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "user":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }

        }
    ],
    "routes":[
        {

            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {

            "src":"/(.*)",
            "dest":"core/wsgi.py"
        }
    ]
}